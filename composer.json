{
    "name": "ngabor84/lumen-api-query-parser",
    "description": "A query parser for REST-APIs based on laravel's lumen framework",
    "keywords": ["lumen", "query parser", "api"],
    "homepage": "https://github.com/ngabor84/lumen-api-query-parser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gábor Németh",
            "email": "negabor@gmail.com",
            "homepage": "https://nemeth-gabor.hu",
            "role": "developer"
        }
    ],
    "require": {
    	"php": "^7.3",
        "laravel/lumen-framework": "^6.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.4",
        "nunomaduro/phpinsights": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "LumenApiQueryParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
